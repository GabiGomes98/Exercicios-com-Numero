{
function SomaValor(numero1, numero2)
    let maiorQueDez = false
    let resultadoSoma = numero1 + numero2
    
    if(resultadoSoma > 10){
        return maiorQueDez = true
    }
    return resultadoSoma
}

console.log("2º resultado:", SomaValorCondicional(7,8))

